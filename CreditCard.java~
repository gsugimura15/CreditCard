public class CreditCard
{
  public boolean verify(string num)  
  {
    if (num.length >= 12 && num.length <= 16)
    {
      int evens = 0;
      int odds = 0;
     for (int i = num.length()-1; i > -1; i-=2)
      {
       int g = Integer.parseInt(num.substring (i,i+1));
       g = g * 2
       if (g >= 10)
       {
         num.substring(0,1) + num.substring(1,2);
         evens += g
       }
     }
          
     for (int i = num.length()-2; i > -1; i-=2)
      {
        int g = Integer.parseInt(num.substring (i,i+1));
       {
         num.substring(0,1) + num.substring(1,2);
         odds += g
       }
     }